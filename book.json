{
  "title": "Ariescat Awesome",
  "author": "Ariescat",
  "description": "记录一些令人秃头的知识",
  "links": {
    "sidebar": {
      "Home": "/"
    }
  },
  "plugins": [
    "-lunr",
    "-search",
    "search-pro",
    "github",
    "code",
    "popup",
    "splitter",
    "chapter-fold",
    "back-to-top-button",
    "page-toc-button",
    "tbfed-pagefooter",
    "hide-element"
  ],
  "pluginsConfig": {
    "github": {
      "url": "https://github.com/Ariescat/Metis-Awesome/"
    },
    "page-toc-button": {
      "maxTocDepth": 2,
      "minTocSize": 2
    },
    "tbfed-pagefooter": {
      "copyright": "Copyright &copy Ariescat",
      "modify_label": "最后修改时间：",
      "modify_format": "YYYY-MM-DD HH:mm"
    },
    "hide-element": {
      "elements": [
        ".gitbook-link"
      ]
    }
  }
}